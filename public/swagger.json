{
  "openapi": "3.0.0",
  "info": {
    "title": "Rafzhost API",
    "version": "1.0.0",
    "description": "Dokumentasi API Rafzhost menggunakan Swagger UI"
  },
  "servers": [
    { "url": "https://rafzhost-api.vercel.app/api", "description": "Server default Vercel" }
  ],
  "paths": {
    "/downloader/tiktok": {
      "get": {
        "tags": ["Downloader"],
        "summary": "Download TikTok video tanpa watermark",
        "parameters": [
          { "in": "query", "name": "url", "schema": { "type": "string" }, "required": true, "description": "URL video TikTok" }
        ],
        "responses": {
          "200": { "description": "Berhasil" },
          "400": { "description": "Parameter URL hilang" },
          "500": { "description": "Error server" }
        }
      }
    }
  }
}
