{
  "openapi": "3.0.0",
  "info": {
    "title": "Rafzhost API",
    "version": "1.0.0",
    "description": "Dokumentasi API Rafzhost menggunakan Swagger UI"
  },
  "servers": [
    {
      "url": "https://rafzhost-api.vercel.app/api",
      "description": "Server default Vercel"
    }
  ],
  "paths": {
    "/ttdl": {
      "get": {
        "tags": ["Downloader"],
        "summary": "Download TikTok video/photo (TikWM)",
        "parameters": [
          {
            "in": "query",
            "name": "url",
            "schema": { "type": "string" },
            "required": true,
            "description": "URL TikTok"
          }
        ],
        "responses": {
          "200": { "description": "Berhasil" },
          "400": { "description": "URL tidak ada atau kosong" },
          "500": { "description": "Error server" }
        }
      }
    }
  }
}
